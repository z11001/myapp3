<h1>ログイン画面</h1>

<%= form_tag users_path do %>
<table id="login_form">
    <tr>
    <td><%= label_tag :account, 'アカウント名' %></td>
    <td><%= text_field_tag :account, params[:account] %></td>
</tr>

<tr>
    <td><%= label_tag :pass, 'パスワード' %></td>
    <td><%= password_field_tag :pass, params[:pass] %></td>
</tr>

<tr>
    <td><%= submit_tag "ログイン" %></td>
</tr>
</table>

<% end %>
